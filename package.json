{
  "name": "react-figma-extensions-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "run-s dev",
    "dev": "run-s watch",
    "dev:ui-only": "vite -c ./vite.config.ui.ts",
    "watch": "run-p watch:*",
    "watch:ui": "npm run build:ui -- --watch --mode=development",
    "watch:plugin": "npm run build:plugin -- --watch --mode=development",
    "clean": "rimraf dist/*",
    "types": "run-s types:*",
    "types:src": "tsc",
    "types:node": "tsc -P tsconfig.node.json",
    "build": "run-s build:*",
    "prebuild": "run-s types clean",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts"
  },
  "dependencies": {
    "@figma/plugin-typings": "^1.93.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "^20.12.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "isbot": "^4.4.0",
    "lucide-react": "^0.379.0",
    "monorepo-networker": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "react-router-dom": "^6.23.1",
    "rimraf": "^5.0.7",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-generate-file": "^0.1.1",
    "vite-plugin-react-rich-svg": "^1.1.1",
    "vite-plugin-singlefile": "^2.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.11"
  }
}
